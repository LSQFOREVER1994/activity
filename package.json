    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.12.4",
    "fs-extra": "^11.2.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest-puppeteer": "^4.0.0",
    "less": "^3.9.0",
    "lint-staged": "^8.1.1",
